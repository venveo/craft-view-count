{
    "name": "venveo/view-count",
    "description": "Keep track of view counts over a period of time to show popular entries.",
    "type": "craft-plugin",
    "version": "1.0.2",
    "keywords": [
        "craft",
        "cms",
        "craftcms",
        "craft-plugin",
        "popular entries"
    ],
    "support": {
        "docs": "https://github.com/venveo/craft-view-count/blob/master/README.md",
        "issues": "hhttps://github.com/venveo/craft-view-count/issues"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Venveo",
            "homepage": "https://www.venveo.com"
        }
    ],
    "require": {
        "craftcms/cms": "^3.0.0"
    },
    "autoload": {
        "psr-4": {
          "venveo\\viewcount\\": "src/"
        }
    },
    "extra": {
        "name": "View Count",
        "handle": "view-count",
        "hasCpSettings": true,
        "hasCpSection": false,
        "changelogUrl": "https://raw.githubusercontent.com/venveo/craft-view-count/master/CHANGELOG.md",
        "components": {
            "service": "venveo\\viewcount\\services\\ViewCount"
        },
        "class": "venveo\\viewcount\\ViewCount"
    }
}
